{
  "optionalDependencies": {
    "lightningcss-linux-x64-gnu": "^1.30.1"
  },
  "name": "tullyelly",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "predev": "npm run gen:build-info",
    "postinstall": "prisma generate",
    "prebuild": "npm run gen:build-info",
    "build": "prisma generate && next build",
    "deploy:prod": "vercel build && vercel deploy --prebuilt --prod",
    "postbuild": "npm run gen:build-info",
    "start": "next start",
    "start:prod:local": "npm run build && cross-env NODE_ENV=production PORT=4010 npm start",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx,json,yml,yaml,css}\"",
    "format:check": "node scripts/check-formatting.mjs",
    "lint": "eslint . --cache --cache-location .cache/eslint/.eslintcache",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "images:optimize": "node scripts/optimize-images.mjs",
    "images:check": "node scripts/check-images.mjs",
    "new-page": "node scripts/new-page.mjs",
    "refactor:flowers": "tsx scripts/refactor-flowers.ts",
    "refactor:flowers:write": "tsx scripts/refactor-flowers.ts --write",
    "validate-frontmatter": "node scripts/validate-frontmatter.mjs",
    "validate-seo": "node scripts/validate-seo.mjs",
    "ensure:diag-off": "node -e \"if(process.env.NEXT_PUBLIC_HYDRATION_DIAG && process.env.NEXT_PUBLIC_HYDRATION_DIAG !== '0'){console.error('Hydration diagnostics must be disabled in prod');process.exit(1)}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "pretypecheck": "node scripts/gen-build-info.mjs",
    "deadcode": "ts-prune",
    "guard:self-fetch": "tsx scripts/assert-no-self-fetch.ts",
    "security-headers:check": "node scripts/assert-security-headers.mjs",
    "secrets:scan": "secretlint \"**/*\"",
    "secrets:scan:staged": "secretlint",
    "share:generate": "ts-node --esm scripts/generateShareKit.ts",
    "coverage:check": "node scripts/report-coverage.mjs",
    "test": "cross-env NODE_ENV=test jest --config jest.config.cjs",
    "pretest": "npm run gen:build-info",
    "test:smoke": "node scripts/jest-related-smoke.mjs",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --config jest.config.cjs",
    "pretest:coverage": "npm run gen:build-info",
    "test:coverage:ci": "cross-env NODE_ENV=test jest --runInBand --coverage --config jest.config.cjs",
    "test:ci": "npm run test:smoke && npm run test:coverage:ci",
    "preci": "npm rebuild lightningcss --foreground-scripts && SHARP_IGNORE_GLOBAL_LIBVIPS=1 npm rebuild sharp --foreground-scripts",
    "pretest:e2e": "npm run gen:build-info && node -e \"require('fs').mkdirSync('.pw-browsers',{recursive:true})\"",
    "test:e2e": "cross-env PLAYWRIGHT_BROWSERS_PATH=.pw-browsers npx playwright test",
    "test:e2e:headed": "cross-env PLAYWRIGHT_BROWSERS_PATH=.pw-browsers npx playwright test --headed",
    "test:e2e:install": "cross-env PLAYWRIGHT_BROWSERS_PATH=.pw-browsers npx playwright install --with-deps",
    "test:e2e:ci": "cross-env PLAYWRIGHT_BROWSERS_PATH=.pw-browsers npx playwright test --reporter=html",
    "db:ping": "DOTENV_CONFIG_PATH=.env.local tsx -r dotenv/config scripts/db-ping.ts",
    "sanity:db": "node scripts/db_sanity.mjs",
    "check:use-server": "bash scripts/check-use-server.sh",
    "gen:build-info": "node scripts/gen-build-info.mjs",
    "prepare": "husky",
    "check:emdash": "node scripts/check-emdash.mjs"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@sentry/nextjs": "^10.8.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "lucide-react": "^0.544.0",
    "next": "^15.5.2",
    "next-auth": "^4.24.11",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.34.3",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/css": "^0.10.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.2.0",
    "@fontsource-variable/inter": "^5.2.6",
    "@fontsource-variable/jetbrains-mono": "^5.2.6",
    "@mdx-js/react": "^3.1.0",
    "@next/eslint-plugin-next": "^15.5.2",
    "@next/mdx": "^15.5.0",
    "@playwright/test": "^1.55.0",
    "@secretlint/secretlint-rule-preset-recommend": "^11.2.4",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.1",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "globby": "^14.0.2",
    "gray-matter": "^4.0.3",
    "husky": "^9.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "jiti": "^2.5.1",
    "lint-staged": "^15.2.0",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "remark-directive": "^3.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^5.2.0",
    "secretlint": "^11.2.4",
    "secretlint-rule-custom-pg-url": "file:tools/secretlint-rules",
    "server-only": "^0.0.1",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "undici": "^7.16.0"
  },
  "lint-staged": {
    "*": [
      "npm run secrets:scan:staged --"
    ],
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,mdx}": [
      "prettier --write",
      "node scripts/fix-md-semicolons.mjs"
    ],
    "*.{json,yml,yaml,css}": "prettier --write"
  }
}
